/* Programa que muestra las tablas de multiplicar hasta el 10
 * 
 */
package bucles;

import java.util.*;

public class bucles6 {

	public static void main(String[] args) {

		Scanner entrada = new Scanner(System.in);

		int i = 1;
		int m = 1;

		while (i <= 10) {
			System.out.println(m + " x " + i + " = " + m*i);
			i++;
			m = m + 1;
			
			
		}
	}
}
