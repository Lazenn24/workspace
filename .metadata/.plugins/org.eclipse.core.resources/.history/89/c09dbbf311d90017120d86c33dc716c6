package Arrays;

public class Prueba02 {

	public static void main(String[] args) {
		
		//Declarar un array cuando no sabemos todavia su contenido
		
		//Declaramos un array de floats con 9 posiciones
		
		float temperaturas[] = new float[9];
		
		String nombres[] = new String[5];
		
		//Rellenamos el array de temperaturas
		temperaturas[0] = 20;
		temperaturas[1] = 22; 
		temperaturas[2] = 20;
		temperaturas[3] = 19;
		temperaturas[4] = 17;
		temperaturas[5] = 17;
		temperaturas[6] = 15;
		temperaturas[7] = 16;
		
		//Rellenamos el array de nombres
		nombres[0] = "Alejandro";
		nombres[1] = "Ram√≥n";
		nombres[2] = "Pablo";
		nombres[3] = "Ankush";
				
		
		

	}

