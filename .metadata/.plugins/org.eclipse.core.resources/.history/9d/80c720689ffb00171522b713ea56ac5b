package leccion04;

public class Rectangulo {
	
	int x, y; // Posición del rectangulo (esquina superior izquierda)
	int altura, base;
	String color;
	
	void mostrar(){
		System.out.println("El rectángulo esta en la posición (" + x + ", " + y + "), tiene base " + base + " y altura " + altura + " y color " + color);
	}

}
