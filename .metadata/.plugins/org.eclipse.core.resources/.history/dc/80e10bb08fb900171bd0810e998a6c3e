package bucles;
import java.util.*;
public class bucles13 {
	
	/* Programa que pide 5 numeros entre 1 y 10.
	 * Si el dato introducido no esta entre 1 y 10 o no es un entero,
	 * vuelve a solicitarlo.
	 */

	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		int nota;
		int i = 1; // COntador de nยบ de pregunta.
		while (i <= 5) {
			System.out.println("Escribe un numero entre el 1 y el 10");
			nota = entrada.nextInt();
			i++;
			while (nota > 10){
				System.out.println("Incorrecto, escribe una nota entre el 1 y el 10");
			}
			
			
		}

	}

}
