package leccion02;

public class Main {
	
	public static void main(String[] args) {
		
		Punto punto1 = new Punto();
		punto1.x = 0;
		punto1.y = 0;
		punto1.color = "verde";
		
		punto1.mostrar();
		
		Punto punto2 = new Punto();
		punto2.x = 25;
		punto2.y = 78;
		punto2.color = "rojo";
		
		punto2.mostrar();
		
		
		
		
	}

}
